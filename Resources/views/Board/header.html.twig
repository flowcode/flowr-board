<div>
    <small>
        {% if filter.projectIteration %}
            {% if filter.projectIteration.project.account %}
                <a href="{{ path('account_show', { 'id': filter.projectIteration.project.account.id }) }}">{{ filter.projectIteration.project.account|capitalize }}</a>
                <i class="fa fa-angle-right"></i>
            {% endif %}
            <a href="{{ path('project_iteration_show', { 'id': filter.projectIteration.id }) }}">{{ filter.projectIteration|capitalize }}</a>
        {% endif %}
    </small>
</div>